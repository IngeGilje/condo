<!DOCTYPE html>
<html lang="nb-NO">

<head>

  <title>Budsjett</title>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="styles/condo.css">

</head>

<body>
  <div class="div-grid-budgets-search">

    <!-- Search consist of 3 columns -->
    <div class="div-budgets"></div>
    <div class="div-budgets-filterAccountId grid-column2"></div>
    <div class="div-budgets-filterYear grid-column3"></div>
  </div>

  <!-- All rows consist of 5 columns -->
  <div class="div-grid-budgets">

    <div class="div-menu grid-column1"></div>
    <div class="div-budgets-budgetId"></div>
    <div class="div-budgets-columnLine grid-column3"></div>
    <div class="div-budgets-columnAccountName grid-column4"></div>
    <div class="div-budgets-columnAmount grid-column5"></div>

    <div class="div-budgets-budgetId"></div>

    <div class="div-budgets-accountId"></div>

    <div class="div-budgets-year"></div>

    <div class="div-budgets-amount"></div>

    <div class="div-budgets-update"></div>

    <div class="div-budgets-insert"></div>

    <div class="div-budgets-delete"></div>

    <div class="div-budgets-cancel"></div>

    <div class="div-bankaccounts-login"></div>
  </div>

  <script src="class/condo-class.js"></script>
  <script src="class/condo-users-class.js"></script>
  <script src="class/condo-accounts-class.js"></script>
  <script src="class/condo-budgets-class.js"></script>
  <script src="scripts/condo-budgets.js"></script>
</body>

</html>